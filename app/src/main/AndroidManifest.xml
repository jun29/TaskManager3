<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jyun0.taskmanager">

    <uses-permission
        android:name="android.permission.READ_PHONE_STATE"
        tools:node="remove" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/taskmanager"
        android:label="@string/app_name"
        android:roundIcon="@drawable/taskmanager"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="jyun0.taskmanager.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="jyun0.taskmanager.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="jyun0.taskmanager.LoginActivity" />
        <activity android:name="jyun0.taskmanager.SignUpActivity" />
    </application>

</manifest>